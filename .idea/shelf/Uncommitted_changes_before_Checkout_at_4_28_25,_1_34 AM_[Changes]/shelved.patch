+                            <div class="col-md-3">
+                                <div class="form-check mb-3">
+                                    {{ form.is_manufacturable(class="form-check-input") }}
+                                    <label class="form-check-label" for="is_manufacturable">
+                                        Производимый
+                                    </label>
+                                </div>
+                            </div>
+                        </div>
+                    </div>
+                </div>
+
+                <div class="mt-4">
+                    <a href="{{ url_for('articles.index') }}" class="btn btn-secondary">
