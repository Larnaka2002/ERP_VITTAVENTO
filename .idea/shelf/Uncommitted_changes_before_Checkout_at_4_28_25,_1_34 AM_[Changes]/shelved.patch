+from flask import Blueprint, render_template, redirect, url_for, flash, request, jsonify
+from app import db
+from app.models.article import Article, ArticleCategory, ArticleAttribute, ArticleAttributeValue, ArticleMedia
+from app.forms.article_forms import ArticleForm, ArticleCategoryForm, ArticleAttributeForm, ArticleMediaForm
+import os
+from werkzeug.utils import secure_filename
+from datetime import datetime
+
+articles_bp = Blueprint('articles', __name__, url_prefix='/articles')
+
+
+# Список артикулов
+@articles_bp.route('/', methods=['GET'])
+def index():
+    page = request.args.get('page', 1, type=int)
+    per_page = request.args.get('per_page', 20, type=int)
+
+    # Фильтры
+    category_id = request.args.get('category_id', type=int)
+    is_active = request.args.get('is_active', type=str)
+    search = request.args.get('search', type=str)
+
+    # Запрос
+    query = Article.query
+
+    # Применение фильтров
+    if category_id:
+        query = query.filter(Article.category_id == category_id)
+
+    if is_active:
+        is_active_bool = (is_active.lower() == 'true')
+        query = query.filter(Article.is_active == is_active_bool)
+
+    if search:
+        query = query.filter(
+            (Article.name.ilike(f'%{search}%')) |
+            (Article.article_code.ilike(f'%{search}%'))
+        )
+
+    # Пагинация
+    articles = query.order_by(Article.created_at.desc()).paginate(
+        page=page, per_page=per_page, error_out=False
+    )
+
+    # Получение категорий для фильтрации
+    categories = ArticleCategory.query.order_by(ArticleCategory.name).all()
+
+    return render_template(
+        'articles/index.html',
+        articles=articles,
+        categories=categories,
+        current_filters={
+            'category_id': category_id,
+            'is_active': is_active,
+            'search': search
+        }
+    )
+
+
+# Просмотр артикула
+@articles_bp.route('/<int:id>', methods=['GET'])
+def view(id):
+    article = Article.query.get_or_404(id)
+    return render_template('articles/view.html', article=article)
+
+
+# Создание артикула
+@articles_bp.route('/create', methods=['GET', 'POST'])
+def create():
+    form = ArticleForm()
+
+    if form.validate_on_submit():
+        # Создание артикула
+        article = Article(
+            name=form.name.data,
+            description=form.description.data,
+            category_id=form.category_id.data,
+            weight=form.weight.data,
+            length=form.length.data,
+            width=form.width.data,
+            height=form.height.data,
+            is_active=form.is_active.data,
+            is_purchasable=form.is_purchasable.data,
+            is_sellable=form.is_sellable.data,
+            is_manufacturable=form.is_manufacturable.data
+        )
+
+        # Генерация кода артикула
+        category = ArticleCategory.query.get(form.category_id.data)
+        if category:
+            # Получение последнего артикула в категории
+            last_article = Article.query.filter(
+                Article.category_id == category.id
+            ).order_by(Article.id.desc()).first()
+
+            # Определение нового номера
+            if last_article:
+                # Разбор последнего кода артикула для получения номера
+                try:
+                    prefix_len = len(category.article_prefix)
+                    last_num = int(last_article.article_code[prefix_len:])
+                    new_num = last_num + 1
+                except:
+                    # Если не удалось разобрать, начинаем с начального значения
+                    new_num = category.sequence_start
+            else:
+                new_num = category.sequence_start
+
+            # Формирование кода артикула
+            article.article_code = f"{category.article_prefix}{new_num:06d}"
+
+        db.session.add(article)
+        db.session.commit()
+
+        flash('Артикул успешно создан.', 'success')
+        return redirect(url_for('articles.view', id=article.id))
+
+    return render_template('articles/create.html', form=form)
+
+
+# Редактирование артикула
+@articles_bp.route('/<int:id>/edit', methods=['GET', 'POST'])
+def edit(id):
+    article = Article.query.get_or_404(id)
+    form = ArticleForm(obj=article)
+
+    if form.validate_on_submit():
+        form.populate_obj(article)
+        article.updated_at = datetime.utcnow()
+
+        db.session.commit()
+        flash('Артикул успешно обновлен.', 'success')
+        return redirect(url_for('articles.view', id=article.id))
+
+    return render_template('articles/edit.html', form=form, article=article)
+
+
+# Удаление артикула
+@articles_bp.route('/<int:id>/delete', methods=['POST'])
+def delete(id):
+    article = Article.query.get_or_404(id)
+
+    try:
+        db.session.delete(article)
+        db.session.commit()
+        flash('Артикул успешно удален.', 'success')
+    except Exception as e:
+        db.session.rollback()
+        flash(f'Ошибка при удалении артикула: {str(e)}', 'danger')
+
+    return redirect(url_for('articles.index'))
+
+
+# Загрузка медиафайла для артикула
+@articles_bp.route('/<int:id>/media', methods=['GET', 'POST'])
+def upload_media(id):
+    article = Article.query.get_or_404(id)
+    form = ArticleMediaForm()
+
+    if form.validate_on_submit():
+        file = form.file.data
+        filename = secure_filename(file.filename)
+
+        # Создание директории для сохранения файлов, если она не существует
+        upload_dir = os.path.join('app', 'static', 'uploads', 'articles', str(article.id))
+        os.makedirs(upload_dir, exist_ok=True)
+
+        # Сохранение файла
+        file_path = os.path.join(upload_dir, filename)
+        file.save(file_path)
+
+        # Создание записи в базе данных
+        relative_path = os.path.join('uploads', 'articles', str(article.id), filename)
+        media = ArticleMedia(
+            article_id=article.id,
+            media_type=form.media_type.data,
+            file_path=relative_path,
+            file_name=filename
+        )
+
+        db.session.add(media)
+        db.session.commit()
+
+        flash('Файл успешно загружен.', 'success')
+        return redirect(url_for('articles.view', id=article.id))
+
+    return render_template('articles/upload_media.html', form=form, article=article)
+
+
+# Маршруты для работы с категориями артикулов
+
+# Список категорий артикулов
+@articles_bp.route('/categories', methods=['GET'])
+def category_index():
+    categories = ArticleCategory.query.order_by(ArticleCategory.name).all()
+    return render_template('articles/categories/index.html', categories=categories)
+
+
+# Создание категории артикулов
+@articles_bp.route('/categories/create', methods=['GET', 'POST'])
+def category_create():
+    form = ArticleCategoryForm()
+
+    if form.validate_on_submit():
+        # Проверка, что код категории уникален
+        existing = ArticleCategory.query.filter_by(code=form.code.data).first()
+        if existing:
+            flash('Категория с таким кодом уже существует.', 'danger')
+            return render_template('articles/categories/create.html', form=form)
+
+        # Создание категории
+        category = ArticleCategory(
+            name=form.name.data,
+            code=form.code.data,
+            article_prefix=form.article_prefix.data,
+            sequence_start=form.sequence_start.data
+        )
+
+        # Установка родительской категории, если выбрана
+        if form.parent_id.data > 0:
+            category.parent_id = form.parent_id.data
+
+        db.session.add(category)
+        db.session.commit()
+
+        flash('Категория успешно создана.', 'success')
+        return redirect(url_for('articles.category_index'))
+
+    return render_template('articles/categories/create.html', form=form)
+
+
+# Маршруты для работы с атрибутами артикулов
+
+# Список атрибутов артикулов
+@articles_bp.route('/attributes', methods=['GET'])
+def attribute_index():
+    attributes = ArticleAttribute.query.order_by(ArticleAttribute.name).all()
+    return render_template('articles/attributes/index.html', attributes=attributes)
+
+
